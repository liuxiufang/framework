{
	"require": {
		"smarty/smarty": "3.1.21"
	},
  "autoload": {
    "classmap": [
      "database"
    ],
	"psr-4": {
	  "App\\": "app/"
	},
    "autoload-dev": {
      "classmap": [
        "tests/TestCase.php"
      ]
    },
    "config": {
      "preferred-install": "dist",
      "secure-http": false
    },
    "repositories": {
      "packagist": {
        "type": "composer",
        "url": "https://packagist.phpcomposer.com"
      }
    }
  }
}
